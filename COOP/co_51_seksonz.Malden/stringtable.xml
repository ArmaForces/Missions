<?xml version="1.0" encoding="utf-8"?>
<Project name="AFP">
    <Package name="Scripts">
        <Key ID="STR_AFP_Scripts_DisplayName">
            <Polish>SEKSONZ - Dosadašnje Sranje</Polish>
        </Key>
        <Key ID="STR_AFP_Scripts_Mission_Description">
            <English>Late afternoon 24 may 1994. Swedish Expeditionary UN Corpse is tasked with route secure task.</English>
            <Polish>Późne popołudnie 24 maja 1994 roku. Szwedzki Ekspedycyjny Korpus Sił ONZ ma zabezpieczyć trasę konwoju.</Polish>
        </Key>

        <!--
            RTB action handling
        -->

        <Key ID="STR_AFP_Scripts_StartRTB">
            <English>Order return to base</English>
            <Polish>Zarządź powrót do bazy</Polish>
        </Key>
        <Key ID="STR_AFP_Scripts_FinishRTB">
            <English>Finish return to base</English>
            <Polish>Zakończ powrót do bazy</Polish>
        </Key>

        <!--
            Tasks
        -->

        <Key ID="STR_AFP_Scripts_Task_RescueCivilians_Title">
            <English>Protect civilian population</English>
            <Polish>Chroń ludność cywilną</Polish>
        </Key>
        <Key ID="STR_AFP_Scripts_Task_RescueCivilians_Description">
            <English>All encountered civilians shall be protected against possible attacks of various military threats. When you secure the area, they will be taken care of by different unit. </English>
            <Polish>Wszystkich napotkanych cywili musicie chronić przed zagrożeniem ze strony innych wojsk w regionie. Gdy zabezpieczycie teren, inna jednostka zajmie się dostarczeniem pomocy humanitarnej.</Polish>
        </Key>

        <Key ID="STR_AFP_Scripts_Task_GravesInvestigation_Title">
            <English>Investigate graves</English>
            <Polish>Zbadaj groby</Polish>
        </Key>
        <Key ID="STR_AFP_Scripts_Task_GravesInvestigation_Description">
            <English>Investigate graves and check if you can find more around.</English>
            <Polish>Przeprowadź wizję lokalną grobów. Spróbuj znaleźć więcej.</Polish>
        </Key>

        <Key ID="STR_AFP_Scripts_Task_ArrestGeneral_Title">
            <English>Arrest general</English>
            <Polish>Aresztuj generała</Polish>
        </Key>
        <Key ID="STR_AFP_Scripts_Task_ArrestGeneral_Description">
            <English>Commander of local Croatian forces will know more about our graves stuff. We need him alive at HQ ASAP.</English>
            <Polish>Dowódca lokalnych sił chorwackich na pewno coś wie o tych grobach. Potrzebujemy dostarczyć go żywcem do bazy.</Polish>
        </Key>

        <Key ID="STR_AFP_Scripts_Task_RTB_Title">
            <English>Return to base</English>
            <Polish>Powrót do bazy</Polish>
        </Key>
        <Key ID="STR_AFP_Scripts_Task_RTB_Description">
            <English>All units must return to base ASAP. Ending this task is possible for highest ranked person among alive ones, using satcom in base.</English>
            <Polish>Wszystkie jednostki mają powrócić do bazy. Zakończenie zadania jest możliwe przez osoby najwyższe rangą spośród żyjących, przy użyciu satcomu w bazie.</Polish>
        </Key>

        <!--
            Mission ending
        -->

        <Key ID="STR_AFP_Scripts_Debriefing_Success_Title">
            <English>Mission Completed!</English>
            <Polish>Misja ukończona!</Polish>
        </Key>
        <Key ID="STR_AFP_Scripts_Debriefing_Success_Subtitle">
            <English>Successfully rescued civilians and secured convoy route.</English>
            <Polish>Ewakuowaliśmy ludność cywilną i zabezpieczyliśmy trasę konwoju.</Polish>
        </Key>

        <!--
            AWACS Stuff
        -->
        <Key ID="STR_AFP_Scripts_AWACS_DetectedMessage">
            <English>Detected %1 on %2 from You. Distance: %3 km</English>
            <Polish>Wykryto %1 na %2 od Twojej pozycji. Odległość: %3 km</Polish>
        </Key>
        <!--
            Rearm script
        -->
        <Key ID="STR_AFP_Scripts_Rearm">
            <English>Rearm interface</English>
            <Polish>Panel przezbrojenia</Polish>
        </Key>
        <Key ID="STR_AFP_Scripts_Remove_Garbage">
            <English>Remove roadblock (requires engineer)</English>
            <Polish>Usuń blokadę (wymaga inżyniera)</Polish>
        </Key>

    </Package>
</Project>











































